package master.logica.clases;

/**
 *
 * @author Javier
 */
public class CTipoSangre {
   private int lgCodigo;
   private String strNombre;

    public CTipoSangre(int lgCodigo, String strNombre) {
        this.lgCodigo = lgCodigo;
        this.strNombre = strNombre;
    }

    public CTipoSangre() {
    }

    public long getLgCodigo() {
        return lgCodigo;
    }

    public void setLgCodigo(int lgCodigo) {
        this.lgCodigo = lgCodigo;
    }

    public String getStrNombre() {
        return strNombre;
    }

    public void setStrNombre(String strNombre) {
        this.strNombre = strNombre;
    }
   
   
}
