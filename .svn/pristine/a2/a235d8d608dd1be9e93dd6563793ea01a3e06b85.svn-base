package control.logica.clases;

import java.sql.Time;

/**
 *
 * @author DiegoPalacios
 */
public class CTipoJornada {

    private int intCodigo;
    private java.sql.Time tmHoraEntrada;
    private java.sql.Time tmHoraSalida;
    private java.sql.Time tmHoraEntradaReceso;
    private java.sql.Time tmHoraSalidaReceso;
    private String strNombre;
    private String strNombreCorto;
    private String strDescripcion;
    private String strObservacion;
    private double dblnumeroHoras;
    private int intEstado;
    private String strDia;

    public CTipoJornada() {
    }

    public CTipoJornada(int intCodigo, Time tmHoraEntrada, Time tmHoraSalida, Time tmHoraEntradaReceso, Time tmHoraSalidaReceso, String strNombre, String strNombreCorto, String strDescripcion, String strObservacion, double dblnumeroHoras, int intEstado, String strDia) {
        this.intCodigo = intCodigo;
        this.tmHoraEntrada = tmHoraEntrada;
        this.tmHoraSalida = tmHoraSalida;
        this.tmHoraEntradaReceso = tmHoraEntradaReceso;
        this.tmHoraSalidaReceso = tmHoraSalidaReceso;
        this.strNombre = strNombre;
        this.strNombreCorto = strNombreCorto;
        this.strDescripcion = strDescripcion;
        this.strObservacion = strObservacion;
        this.dblnumeroHoras = dblnumeroHoras;
        this.intEstado = intEstado;
        this.strDia = strDia;
    }

    public int getIntCodigo() {
        return intCodigo;
    }

    public void setIntCodigo(int intCodigo) {
        this.intCodigo = intCodigo;
    }

    public Time getTmHoraEntrada() {
        return tmHoraEntrada;
    }

    public void setTmHoraEntrada(Time tmHoraEntrada) {
        this.tmHoraEntrada = tmHoraEntrada;
    }

    public Time getTmHoraSalida() {
        return tmHoraSalida;
    }

    public void setTmHoraSalida(Time tmHoraSalida) {
        this.tmHoraSalida = tmHoraSalida;
    }

    public Time getTmHoraEntradaReceso() {
        return tmHoraEntradaReceso;
    }

    public void setTmHoraEntradaReceso(Time tmHoraEntradaReceso) {
        this.tmHoraEntradaReceso = tmHoraEntradaReceso;
    }

    public Time getTmHoraSalidaReceso() {
        return tmHoraSalidaReceso;
    }

    public void setTmHoraSalidaReceso(Time tmHoraSalidaReceso) {
        this.tmHoraSalidaReceso = tmHoraSalidaReceso;
    }

    public String getStrNombre() {
        return strNombre;
    }

    public void setStrNombre(String strNombre) {
        this.strNombre = strNombre;
    }

    public String getStrNombreCorto() {
        return strNombreCorto;
    }

    public void setStrNombreCorto(String strNombreCorto) {
        this.strNombreCorto = strNombreCorto;
    }

    public String getStrDescripcion() {
        return strDescripcion;
    }

    public void setStrDescripcion(String strDescripcion) {
        this.strDescripcion = strDescripcion;
    }

    public String getStrObservacion() {
        return strObservacion;
    }

    public void setStrObservacion(String strObservacion) {
        this.strObservacion = strObservacion;
    }

    public double getDblnumeroHoras() {
        return dblnumeroHoras;
    }

    public void setDblnumeroHoras(double dblnumeroHoras) {
        this.dblnumeroHoras = dblnumeroHoras;
    }

    public int getIntEstado() {
        return intEstado;
    }

    public void setIntEstado(int intEstado) {
        this.intEstado = intEstado;
    }

    public String getStrDia() {
        return strDia;
    }

    public void setStrDia(String strDia) {
        this.strDia = strDia;
    }

}
