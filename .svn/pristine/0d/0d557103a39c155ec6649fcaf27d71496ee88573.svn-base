package master.logica.clases;

/**
 *
 * @author Javier
 */
public class CParroquia {

    private long lgCodigo;
    private String strNombre;
    private CCiudad objCodigoCiudad;
    private boolean blEsUrbana;

    public CParroquia(long lgCodigo, String strNombre, CCiudad objCodigoCiudad, boolean blEsUrbana) {
        this.lgCodigo = lgCodigo;
        this.strNombre = strNombre;
        this.objCodigoCiudad = objCodigoCiudad;
        this.blEsUrbana = blEsUrbana;
    }

    public CParroquia() {
    }

    public long getLgCodigo() {
        return lgCodigo;
    }

    public void setLgCodigo(long lgCodigo) {
        this.lgCodigo = lgCodigo;
    }

    public String getStrNombre() {
        return strNombre;
    }

    public void setStrNombre(String strNombre) {
        this.strNombre = strNombre;
    }

    public CCiudad getObjCodigoCiudad() {
        return objCodigoCiudad;
    }

    public void setObjCodigoCiudad(CCiudad objCodigoCiudad) {
        this.objCodigoCiudad = objCodigoCiudad;
    }

    public boolean getBlEsUrbana() {
        return blEsUrbana;
    }

    public void setBlEsUrbana(boolean blEsUrbana) {
        this.blEsUrbana = blEsUrbana;
    }

}
