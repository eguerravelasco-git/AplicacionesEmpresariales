package master.logica.clases;

/**
 *
 * @author Javier
 */
public class CEstadoCivil {
    private long lgCodigo;
    private String strCodificacion;
    private String strNombre;

    public CEstadoCivil(long lgCodigo, String strCodificacion, String strNombre) {
        this.lgCodigo = lgCodigo;
        this.strCodificacion = strCodificacion;
        this.strNombre = strNombre;
    }

    public CEstadoCivil() {
    }

    public long getLgCodigo() {
        return lgCodigo;
    }

    public void setLgCodigo(long lgCodigo) {
        this.lgCodigo = lgCodigo;
    }

    public String getStrCodificacion() {
        return strCodificacion;
    }

    public void setStrCodificacion(String strCodificacion) {
        this.strCodificacion = strCodificacion;
    }

    public String getStrNombre() {
        return strNombre;
    }

    public void setStrNombre(String strNombre) {
        this.strNombre = strNombre;
    }
    
}
