package master.logica.clases;

/**
 *
 * @author Javier
 */
public class CCiudad {
    private long lgCodigo;
    private String strNombre;
    private CProvincia objCodigoProvincia;

    public CCiudad(long lgCodigo, String strNombre, CProvincia objCodigoProvincia) {
        this.lgCodigo = lgCodigo;
        this.strNombre = strNombre;
        this.objCodigoProvincia = objCodigoProvincia;
    }

    public CCiudad() {
    }

    
    
    public long getLgCodigo() {
        return lgCodigo;
    }

    public void setLgCodigo(long lgCodigo) {
        this.lgCodigo = lgCodigo;
    }

    public String getStrNombre() {
        return strNombre;
    }

    public void setStrNombre(String strNombre) {
        this.strNombre = strNombre;
    }

    public CProvincia getObjProvincia() {
        return objCodigoProvincia;
    }

    public void setObjProvincia(CProvincia objCodigoProvincia) {
        this.objCodigoProvincia = objCodigoProvincia;
    }
    
    
    
    
}
